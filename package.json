{
  "name": "dbapp",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:8080",
  "dependencies": {
    "axios": "^0.21.2",
    "bookshelf": "^0.12.0",
    "concurrently": "^3.5.1",
    "knex": "^0.14.2",
    "pg": "^7.4.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
